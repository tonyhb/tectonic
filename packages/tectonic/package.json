{
  "name": "tectonic",
  "version": "2.0.2",
  "description": "Declarative data loading for React",
  "main": "transpiled/index.js",
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "$(npm bin)/eslint ./src",

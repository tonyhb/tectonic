    "test": "$(npm bin)/mocha --compilers js:babel-register --recursive --require ./test/_setup.js ./test/specs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tonyhb/tectonic.git"
  },
  "keywords": [
    "react",
    "data",
    "loading",
    "redux",
    "declarative",
    "data"
  ],
  "author": "Tony Holdstock-Brown",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/tonyhb/tectonic/issues"
  },
  "homepage": "https://github.com/tonyhb/tectonic#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.0.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.5.0",
